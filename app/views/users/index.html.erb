<div class='user_index'>
    <div class='row'>
        <div class='col-md-10 col-md-offset-1 text-center'>
            <h3>User List</h3>
        </div>
    </div>
    <div class='row'>
        <div class='col-md-10 col-md-offset-1'>
            <div class='row'>
                <div class='col-md-10 col-md-offset-1'>
                    <h3 class='text-center'>Admin</h3>
                </div>
                <%= render "users/user_list", users: @admin %>
            </div>
            <div class='row'>
                <div class='col-md-10 col-md-offset-1'>
                    <h3 class='text-center'>Seller</h3>
                </div>
                <%= render "users/user_list", users: @seller %>
            </div>
            <div class='row'>
                <div class='col-md-10 col-md-offset-1'>
                    <h3 class='text-center'>Standard User</h3>
                </div>
                <%= render "users/user_list", users: @standard_user %>
            </div>
        </div>
    </div>
</div>